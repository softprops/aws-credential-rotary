{
  "name": "aws-credential-rotary",
  "version": "0.1.1",
  "private": true,
  "description": "GitHub Action for rotating aws credential secrets",
  "main": "lib/main.js",
  "scripts": {
    "build": "npx ncc build src/main.ts --minify",
    "buildcheck": "npx tsc --noEmit -p .",
    "test": "npx jest --coverage",
    "fmt": "npx prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "fmtcheck": "npx prettier --check 'src/**/*.ts' '__tests__/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softprops/aws-credential-rotary.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "softprops",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^4.0.0",
    "@aws-sdk/client-iam": "^3.21.0",
    "@aws-sdk/client-sts": "^3.21.0",
    "@octokit/rest": "^18.6.7",
    "tweetsodium": "0.0.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.5",
    "@vercel/ncc": "^0.36.1",
    "aws-sdk-mock": "^5.2.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.1.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "{__tests__,src}/**/*.ts": [
      "npm run fmt"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
